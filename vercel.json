{
  "functions": {
    "api/webhook/email.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/webhook/email",
      "destination": "/api/webhook/email.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "env": {
    "FIREBASE_CLIENT_TIMEOUT": "60000"
  }
}